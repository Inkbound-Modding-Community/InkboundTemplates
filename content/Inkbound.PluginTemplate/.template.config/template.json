{
  "$schema": "http://json.schemastore.org/template",
  "author": "Inkbound Modding Community",
  "classifications": [ "BepInEx", "BepInEx 5" , "Plugin", "Inkbound Plugin" ],
  "identity": "InkboundPlugin",
  "name": "BepInEx 5 Inkbound Plugin",
  "shortName": "inkboundplugin",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "Inkbound.PluginTemplate",
  "symbols": {
    "TargetFramework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "text",
      "defaultValue": "net472",
      "replaces": "net472"
    },
    "Description": {
      "type": "parameter",
      "description": "Plugin description",
      "datatype": "text",
      "replaces": "Template Plugin",
      "defaultValue": "Template Plugin"
    },
    "UnityVersion": {
      "type": "parameter",
      "description": "Unity version to use when developing the plugin",
      "datatype": "text",
      "replaces": "2021.3.29",
      "defaultValue": "2021.3.29"
    },
    "Version": {
      "type": "parameter",
      "description": "Plugin version",
      "datatype": "text",
      "defaultValue": "1.0.0"
    },
    "VersionImpl": {
      "type": "generated",
      "generator": "join",
      "replaces": "<Version>1.0.0</Version>",
      "parameters": {
        "symbols": [
          {
            "type": "const",
            "value": "<Version>"
          },
          {
            "type": "ref",
            "value": "Version"
          },
          {
            "type": "const",
            "value": "</Version>"
          }
        ]
      }
    }
  }
}